{
  "name": "wasm_boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^4.0.2",
    "react-vis": "^1.11.7",
    "tsconfig": "^7.0.0",
    "tslint": "^6.1.3",
    "tsutils": "^3.20.0",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "setup-rust": "node bin/setup.js",
    "build-wasm": "cd wasm && wasm-pack build",
    "link-wasm": "cd wasm/pkg && npm link && cd ../ && npm link wasm",
    "start": "./node_modules/.bin/react-app-rewired start",
    "build": "./node_modules/.bin/react-app-rewired build",
    "test": "./node_modules/.bin/react-app-rewired test",
    "eject": "./node_modules/.bin/react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.3",
    "wasm-loader": "^1.3.0"
  }
}
